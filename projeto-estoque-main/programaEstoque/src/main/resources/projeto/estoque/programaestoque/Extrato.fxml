        <columns>
            <TableColumn styleClass="labelTAB" prefWidth="75.0" text="Nome" />
            <TableColumn styleClass="labelTAB" prefWidth="75.0" text="ID" />
            <TableColumn styleClass="labelTAB" prefWidth="66.0" text="UN" />
            <TableColumn styleClass="labelTAB" prefWidth="75.0" text="Quantidade" />
